{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@5.1.3",
      "destination": "wwwroot/twitter-bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/jquery/"
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/toastr/toastr.js/",
      "files": [
        "toastr.min.css"
      ]
    }
  ]
}